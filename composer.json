{
    "name": "norman-huth/nova-watch",
    "type": "project",
    "description": "Watch Laravel Nova releases and file changes. Created with norman-huth/advanced-laravel-installer.",
    "support": {
        "source": "https://github.com/Muetze42/nova-watch",
        "docs": "https://github.com/Muetze42/nova-watch#readme",
        "issues": "https://github.com/Muetze42/nova-watch/issues"
    },
    "keywords": [
        "laravel",
        "laravel-nova",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.3",
        "laravel/socialite": "^5.10",
        "laravel/tinker": "^2.8",
        "socialiteproviders/github": "^4.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.9.1",
        "inertiajs/inertia-laravel": "^0.6.11",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "norman-huth/helpers-collection-laravel": "^1.1",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "functions/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "./vendor/bin/pint"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "code-quality": [
            "./vendor/bin/pint",
            "phpmd app,database,config,routes text phpmd.xml"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/nova"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
